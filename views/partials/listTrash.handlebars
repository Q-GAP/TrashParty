 <div class="trashCard col s12 m6 l4">
      <div class="card teal
      {{#if (if_eq trash.rarity 1)}}
      rare
      {{/if}}
      {{#if (if_eq trash.rarity 2)}}
      uncommon
      {{/if}}
      {{#if (if_eq trash.rarity 3)}}
      common
      {{/if}}">
        <div class="card-image">
          <img class="trashImage{{id}}" src="{{stillGif trash.image}}">
          <span class="card-title">{{trash.name}}</span>
        </div>
        <div class="card-content">
          <p class="category">Category: {{trash.category}}</p>
        </div>
      </div>
    </div>
    <script>
    $(function() {
    $(".trashImage{{id}}").hover(
        function() {
            $(this).attr("src", "{{trash.image}}");
        },
        function() {
            $(this).attr("src", "{{stillGif trash.image}}");
        }                         
    );                  
});
</script>