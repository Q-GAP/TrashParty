<div id="splash">
    {{#if loggedIn}}
    <h1 class="white-text">Welcome, {{username}}!</h1>
    <h3 class="twoTone"> Remember, <br><span>Stay Trashy!</span></h3>
    {{else}}
    <h1 class="white-text">Welcome to Trash<span class="twoTone">Party</span></h1>
    {{/if}}
    <hr>
<div class="col s12 m7">
    <div class="splash-container">
        {{!-- <div class="row center TrashCollectors">
                <img class="inline-Trashy" src="/images/Trashy.png" alt="Trashy the Racoon,Partying Like He Do."> --}}
                <h2 class=" white-text header">Recent Trash Collectors</h2>

            <div class="row">
                <div class="fiveWall">
                {{#each users as |user|}}
                {{> lastFiveCollectors}}
                {{/each}}
                </div>
            </div>
            <div>
                <blockquote>
                    There is no such thing as garbage, just useful stuff in the wrong place.
                </blockquote>
                <i class="author">Alex Steffen</i>
            </div>
        </div>
    </div>
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
